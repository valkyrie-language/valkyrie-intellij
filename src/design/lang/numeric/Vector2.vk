namespace std.numeric;

class Vector2 {
	x: f32,
	y: f32,
}

extends Vector3: From[f32], From[Vector2], From[Vector4] {
	from(v: f32) {
		return Vector3 {
			x: v,
			y: 0.0,
			z: 0.0,
		};
	}
	from(v: Vector2) {
		return new Vector3 {
			x: v.x,
			y: v.y,
			z: 0.0,
		}
	}
	from(v: Vector4) {
		return new Vector3 {
			x: v.x,
			y: v.y,
			z: v.z,
		}
	}
}