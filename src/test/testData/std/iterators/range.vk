namespace std.iterators;

micro range(max: int)-> Range {
    range(1, max, 1)
}

micro range(min: int, max: int)-> Range {
    range(1, max, 1)
}

micro range(min: i32, max: i32, step: i32)-> Range {
    new Range {
    from = min,
    till = max,
    step = step,
    _current = min
    }
}

class Range {
    from: Integer,
    till: Integer,
    step: Integer,
    _current: Integer,
}

trait Iterator {

}

extends Range: Iterator⟨Integer⟩ {
    next() {}
}

extends Range: RandomIterator {

}
